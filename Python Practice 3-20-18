#Python: Quick Functions Review

def go_bananas(x):
  return str(x) + " is bananas" 

print go_bananas([1,2,3])






print




#Python: Lists and Functions Problem

def speak_spanish(m):
  if len(m)>0:
    m[0] = "hola"
    return
  
m = ["hello","amigo"]
speak_spanish(m)
print m


print



#Python Iterating Over Lists

# You Do Round 3
listy_list = ["raindrops", "roses", "kittens", "kettle", "comet"]
del listy_list[3]
listy_list.append("mittens")
print listy_list

print

# You Do Round 4
last_list = [3, 1, 4]
for x in last_list:
  print x * 2
print last_list
